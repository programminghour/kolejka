#include <iostream>

using namespace std;

int tests  = 0;


string ustaw(string ciag)
{
    bool zostaw[ciag.length()];
    string ostateczny = "";

    for(int i = 0; i < ciag.length(); i++)
        zostaw[i] = true;

    for(int i = ciag.length(); i > 0; i--)
        for(int j = i-1; j >= 0; j--)
            if((int)ciag[i-1]>=(int)ciag[j-1])
                zostaw[j-1] = false;

    for(int i = 0; i < ciag.length(); i++)
        if(zostaw[i])
            ostateczny += ciag[i];
    return ostateczny;
}

int main()
{
    cin >> tests;

    for(int i = 0; i < tests; i++)
    {
        string kolejka;
        cin  >> kolejka;
        cout << ustaw(kolejka) <<endl;

    }

    return 0;
}

